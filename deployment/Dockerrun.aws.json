{
    "AWSEBDockerrunVersion": "2",
    "containerDefinitions": [
        {
            "environment": [
                {
                    "name": "DB_PORT",
                    "value": "3306"
                },
                {
                    "name": "DB_HOST",
                    "value": "database"
                }
            ],
            "essential": true,
            "image": "scottx611x/cscie71termproject_app:latest",
            "mountPoints": [
                {
                    "containerPath": "/var/www",
                    "sourceVolume": "_App-Context"
                }
            ],
            "name": "app",
            "memoryReservation": 512,
            "workingDirectory": "/var/www"
        },
        {
            "environment": [
                {
                    "name": "MYSQL_DATABASE",
                    "value": "homestead"
                },
                {
                    "name": "MYSQL_USER",
                    "value": "homestead"
                },
                {
                    "name": "MYSQL_PASSWORD",
                    "value": "secret"
                },
                {
                    "name": "MYSQL_ROOT_PASSWORD",
                    "value": "secret"
                }
            ],
            "essential": true,
            "image": "mysql:5.6",
            "mountPoints": [
                {
                    "containerPath": "/var/lib/mysql",
                    "sourceVolume": "Dbdata"
                }
            ],
            "name": "database",
            "memoryReservation": 512,
            "portMappings": [
                {
                    "containerPort": 3306,
                    "hostPort": 33061
                }
            ]
        },
        {
            "essential": true,
            "image": "scottx611x/cscie71termproject_web:latest",
            "name": "web",
            "memoryReservation": 512,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 8888
                }
            ],
            "volumesFrom": [
                {
                    "sourceContainer": "app"
                }
            ],
            "workingDirectory": "/var/www"
        }
    ],
    "family": "",
    "volumes": [
        {
            "host": {
                "sourcePath": "./app-context/"
            },
            "name": "_App-Context"
        },
        {
            "host": {
                "sourcePath": "dbdata"
            },
            "name": "Dbdata"
        }
    ]
}
