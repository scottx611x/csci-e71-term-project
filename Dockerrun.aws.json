{
    "AWSEBDockerrunVersion": "2",
    "networkMode": "bridge",
    "containerDefinitions": [
        {
            "environment": [
                {
                    "name": "DB_PORT",
                    "value": "33061"
                },
                {
                    "name": "DB_HOST",
                    "value": "database"
                }
            ],
            "essential": true,
            "image": "scottx611x/cscie71termproject_app:latest",
            "mountPoints": [
                {
                    "containerPath": "/var/www/html",
                    "sourceVolume": "app_context"
                }
            ],
            "name": "app",
            "links": [
              "database"
            ],
            "memory": 1024,
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 9000
                }
            ],
            "workingDirectory": "/var/www/html",
            "command": ["sh", "install_deps.sh"]
        },
        {
            "environment": [
                {
                    "name": "MYSQL_DATABASE",
                    "value": "homestead"
                },
                {
                    "name": "MYSQL_USER",
                    "value": "homestead"
                },
                {
                    "name": "MYSQL_PASSWORD",
                    "value": "secret"
                },
                {
                    "name": "MYSQL_ROOT_PASSWORD",
                    "value": "secret"
                }
            ],
            "essential": true,
            "image": "mysql:5.6",
            "mountPoints": [
                {
                    "containerPath": "/var/lib/mysql",
                    "sourceVolume": "db_data"
                }
            ],
            "name": "database",
            "memory": 1024,
            "portMappings": [
                {
                    "containerPort": 3306,
                    "hostPort": 33061
                }
            ]
        },
        {
            "essential": true,
            "image": "scottx611x/cscie71termproject_web:latest",
            "name": "web",
            "links": [
              "app"
            ],
            "memory": 1024,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "volumesFrom": [
                {
                    "sourceContainer": "app"
                }
            ],
            "workingDirectory": "/var/www"
        }
    ],
    "family": "",
    "volumes": [
        {
            "host": {
                "sourcePath": "/var/app/current/app-context"
            },
            "name": "app_context"
        },
        {
            "host": {
                "sourcePath": "dbdata"
            },
            "name": "db_data"
        }
    ]
}
